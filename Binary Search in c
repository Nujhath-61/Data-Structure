#include<stdio.h>
int t;
int binarySearch(int a[],int low,int high,int target)
{
     while(low<=high)
    {
         int mid = low + (high - low) / 2;
        if(target>a[mid])
        {
          low= mid +1;
        }
      if(target<a[mid])
        {
           high = mid -1;
        }
        if(target == a[mid]){
         return mid;
               }

    }
     return -1;
}
int main ()
{
    int n,found;
    scanf("%d",&n);
    int a[n];
     scanf("%d",&t);
    for(int i = 0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
  
   int result = binarySearch( a,0,n-1,t);
if(result == -1) printf("Element is not present in array");
   else printf("Element found is at index %d",result);
}
